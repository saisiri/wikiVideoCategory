{% extends 'base.html.twig' %}

{% block title %}Successfully added new channel pattern  {% endblock %}

{% block body %}
    <style>
        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    </style>

    <div class="example-wrapper">
        <h3>Successfully added new channel pattern</h3>
        <ul>
            <li>{{ id }}</li>
            <li>{{ needle }}</li>
            <li>{{ pattern }}</li>
            <li>{{ youtube_channel.commonname }} / {{ youtube_channel.youtubeusername }}</li>
        </ul>
    </div>
{% endblock %}
